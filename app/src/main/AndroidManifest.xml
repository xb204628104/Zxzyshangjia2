<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zxtyshangjia.zxzyshangjia">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"></uses-permission>

    <application
        android:name=".app.Myappalication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <!-- 高德地图key -->
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="2de7fd739a9ddd38bd17c6f44b0b2ef3" />

        <!-- 获取权限 -->
        <!-- 地图包、搜索包需要的基础权限 -->
        <!--允许程序打开网络套接字-->
        <uses-permission android:name="android.permission.INTERNET" />
        <!--允许程序设置内置sd卡的写权限-->
        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
        <!--允许程序获取网络状态-->
        <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
        <!--允许程序访问WiFi网络信息-->
        <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
        <!--允许程序读写手机状态和身份-->
        <uses-permission android:name="android.permission.READ_PHONE_STATE" />
        <!--允许程序访问CellID或WiFi热点来获取粗略的位置-->
        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
        <!--用于访问GPS定位-->
        <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
        <!--用于获取wifi的获取权限，wifi信息会用来进行网络定位-->
        <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
        <!--用于读取手机当前的状态-->
        <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
        <!--用于申请调用A-GPS模块-->
        <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS"/>


        <activity android:name=".login.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".control.activity.WithdrawActivity"></activity>
        <activity android:name=".control.activity.MessagelistActivity"></activity>
        <activity android:name=".mine.activity.MySetActivity"></activity>
        <activity android:name=".mine.activity.MyCollectActivity"></activity>
        <activity android:name=".login.activity.ModifyPasswordActivity"></activity>
        <activity android:name=".mine.activity.MyEvaluationActivity"></activity>
        <activity android:name=".mine.activity.MyDrawActivity"></activity>
        <activity android:name=".control.activity.DetailActivity"></activity>
        <activity android:name=".login.activity.RegistActivity"></activity>
        <activity android:name=".login.activity.ForgetPasswordActivity"></activity>
        <activity android:name=".control.activity.BindAccountActivity"></activity>
        <activity android:name=".control.activity.AddAccountActivity"></activity>
        <activity android:name=".control.activity.WithdrawListActivity"></activity>
        <activity android:name=".login.activity.GaodeSearchForActivity"></activity>
    </application>

</manifest>